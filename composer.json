{
    "name": "open-orchestra/open-orchestra-base-bundle",
    "description": "The OpenOrchestra base bundle",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "OpenOrchestra community",
            "homepage": "https://github.com/open-orchestra/open-orchestra-base-bundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : ">=5.4",
        "symfony/symfony" : "2.7.4",
        "twig/extensions" : "1.3.0",
        "twig/twig": "1.22.1",
        "symfony/assetic-bundle" : "2.7.0",
        "symfony/monolog-bundle" : "2.7.1",
        "sensio/distribution-bundle" : "4.0.1",
        "sensio/framework-extra-bundle" : "3.0.10",
        "sensio/generator-bundle" : "2.5.3",
        "snc/redis-bundle" : "1.1.9"
    },
    "require-dev": {
        "psr/log": "1.0.0",
        "codeclimate/php-test-reporter": "dev-master",
        "phpunit/phpunit" : "4.7.2",
        "phake/phake": "2.1.0"
    },
    "config" : {
        "bin-dir" : "bin"
    },
    "autoload": {
        "psr-4": {
            "OpenOrchestra\\BaseBundle\\": "BaseBundle/"
        }
    }
}
